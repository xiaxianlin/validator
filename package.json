{
    "name": "@xiaxianlin/validator",
    "version": "0.0.1",
    "description": " React Component Template",
    "main": "lib/index.js",
    "module": "es/index.js",
    "files": [
        "css",
        "es",
        "lib",
        "umd"
    ],
    "scripts": {
        "build": "nwb build-react-component",
        "build:ts": "tsc",
        "build:all": "npm-run-all copy:css-to-lib build:ts",
