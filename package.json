        "css-loader": "^3.1.0",
        "node-sass": "^4.12.0",
        "nwb": "0.23.x",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
